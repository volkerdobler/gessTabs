{
  "extends": [
    "plugin:prettier/recommended",
    "airbnb",
    "wesbos"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-console": 2,
    "no-unused-vars": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es6",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}

{
  "name": "gesstabs",
  "displayName": "gessTabs",
  "description": "gess.Tabs Support",
  "version": "0.2.6",
  "publisher": "volkerdobler",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:gesstabs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/volkerdobler/gessTabs.git"
  },
  "license": "AFL-3.0",
  "main": "./out/extension",
  "icon": "images/gesstabs_icon.png",
  "contributes": {
    "languages": [
      {
        "id": "gesstabs",
        "aliases": [
          "gessTabs",
          "gess.Tabs"
        ],
        "extensions": [
          ".tab",
          ".inc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "gesstabs",
        "scopeName": "source.gessTabs",
        "path": "./syntaxes/gessTabs.tmLanguage"
      }
    ],
    "snippets": [
      {
        "language": "gesstabs",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1"
  }
}
